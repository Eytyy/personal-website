{
  "name": "website",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "npm run cleanBuild & npm run build:server & npm run build:client",
    "build:server": "webpack --config webpack.config.server.prod.js",
    "build:client": "webpack --config webpack.config.prod.js",
    "dev": "npm run clean & npm run dev:server & npm run dev:client && npm run dev:start",
    "dev:start": "nodemon ./dist/server.js",
    "dev:server": "webpack --config webpack.config.server.dev.js",
    "dev:client": "webpack --config webpack.config.dev.js",
    "clean": "rm -rf dist",
    "cleanBuild": "rm -rf build",
    "start": "node ./build/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "pretty-quick",
    "lint": "eslint ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint && npm run test"
    }
  },
  "keywords": [],
  "author": "Eytyy",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-plugin-jest-dom": "^1.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^2.0.0",
    "react-axe": "^3.3.0",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "@hot-loader/react-dom": "^16.10.2",
    "@testing-library/jest-dom": "^4.2.3",
    "@testing-library/react": "^9.3.0",
    "@u-wave/react-vimeo": "^0.6.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "contentful": "^7.10.0",
    "core-js": "^3.3.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.15",
    "react-router-dom": "^5.1.2",
    "styled-components": "^4.4.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  }
}
